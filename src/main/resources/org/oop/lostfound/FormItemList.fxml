<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="252.0" prefWidth="270.0" style="-fx-background-color: white; -fx-background-radius: 20; -fx-padding: 15;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="24.0" prefWidth="379.0" spacing="40.0">
         <children>
            <Label fx:id="labelName" alignment="TOP_LEFT" prefHeight="20.0" prefWidth="224.0" text="Label">
               <font>
                  <Font name="Arial Bold" size="17.0" />
               </font>
            </Label>
            <Label fx:id="labelStatus" alignment="CENTER" prefHeight="17.0" prefWidth="131.0" style="-fx-background-radius: 5;" text="Label">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
               <padding>
                  <Insets bottom="2.0" left="8.0" right="8.0" top="2.0" />
               </padding>
            </Label>
         </children>
      </HBox>
      <ImageView fx:id="imageView" fitHeight="100.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
      <Label opacity="0.6" prefHeight="9.0" prefWidth="230.0" text="Label" wrapText="true" />
      <GridPane hgap="10.0" prefHeight="80.0" prefWidth="240.0" vgap="5.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="105.0" minWidth="10.0" prefWidth="67.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="155.0" minWidth="10.0" prefWidth="153.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Label prefHeight="16.0" prefWidth="68.0" text="Category:">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="labelCategory" prefHeight="17.0" prefWidth="163.0" text="Label" GridPane.columnIndex="1">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Label>
            <Label fx:id="labelLocation" prefHeight="17.0" prefWidth="163.0" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Label>
            <Label fx:id="labelDate" prefHeight="17.0" prefWidth="163.0" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Label>
            <Label prefHeight="16.0" prefWidth="68.0" text="Location:" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
            <Label prefHeight="16.0" prefWidth="97.0" text="Date:" GridPane.rowIndex="2">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </GridPane>
   </children>
</VBox>
